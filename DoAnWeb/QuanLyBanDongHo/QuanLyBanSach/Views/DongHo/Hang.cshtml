@model IEnumerable<QuanLyBanSach.Models.HANG>
@{
    ViewBag.Title = "Hang";
  
}


<div class="card bg-light mb-3 wow slideInLeft">
                <div class="card-header bg-primary text-white text-uppercase"><i class="fa fa-list"></i> Hãng Đồng Hồ</div>
                <ul class="list-group category_block">
                    @foreach(var item in Model)
{
                    <li class="list-group-item wow bounceInUp"><a href="@Url.Action("SPTheoHang", "DongHo", new { id=item.MaHA})">@item.TenHA</a></li>
                    }
                </ul>
            </div>
